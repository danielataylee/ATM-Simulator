//Card Class
public class Card
{
	private String name; //Name of the user
	private String number; //Card number
	private String pin; //4 pin
	private String bank; //Bank name
	private Account savings;
	private Account chequings;
	
	//Constructor
	public Card (String name, String number, String pin, String bank)
	{
		this.name = name;
		this.number = number;
		this.pin = pin;
		this. bank = bank;	
	}	
}


